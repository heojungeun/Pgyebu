<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pgyebu">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_circle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_circle_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.pgyebu">
        <activity android:name=".view.SplashActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.MainActivity"></activity>
        <activity android:name=".view.signup.SignupActivity"></activity>
        <activity android:name=".view.pwsearch.PwSearchActivity"></activity>
        <activity android:name=".view.home.HomeActivity"></activity>
        <activity android:name=".view.add.AddActivity"></activity>
        <activity android:name=".view.chart.ChartActivity"></activity>
        <activity android:name=".view.mypage.MypagemenuActivity"></activity>
        <activity android:name=".view.setting.SettingmenuActivity"></activity>
        <activity android:name=".view.setting.SetStandardActivity"></activity>
        <activity android:name=".view.setting.MainScreenActivity"></activity>
        <activity android:name=".view.mypage.EditPwActivity"></activity>
        <activity android:name=".view.mypage.EditUserInfoActivity"></activity>
        <activity android:name=".view.mypage.SecessionActivity"></activity>
    </application>

</manifest>